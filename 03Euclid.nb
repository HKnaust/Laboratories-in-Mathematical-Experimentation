Cell["Linear Diophantine Equations", "Subsection",ExpressionUUID->"4b82a697-cea3-49d5-8587-846221ee9ace"],

Cell[TextData[{
 "The command below computes integer solutions to the Linear Diophantine \
Equation  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"a", "  ", "x"}], "+", 
     RowBox[{"b", " ", "y"}]}], "=", 
    RowBox[{"GCD", "(", 
     RowBox[{"a", ",", "b"}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "9186927b-0cd8-46c2-9789-280d7b66f410"],
 "."
}], "Text",
 CellChangeTimes->{3.5564188391364946`*^9, 3.808584179184891*^9},
 TextAlignment->Left,ExpressionUUID->"2bbf0b58-7d7b-4d5c-bbfb-979b0fa9bb1f"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Dioph", "[", 
   RowBox[{"a_", ",", "b_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"a2", "=", 
       RowBox[{"Max", "[", 
        RowBox[{"a", ",", "b"}], "]"}]}], ",", 
      RowBox[{"b2", "=", 
       RowBox[{"Min", "[", 
        RowBox[{"a", ",", "b"}], "]"}]}], ",", 
      RowBox[{"a1", "=", 
       RowBox[{"Max", "[", 
        RowBox[{"a", ",", "b"}], "]"}]}], ",", 
      RowBox[{"b1", "=", 
       RowBox[{"Min", "[", 
        RowBox[{"a", ",", "b"}], "]"}]}], ",", 
      RowBox[{"xold", "=", "1"}], ",", 
      RowBox[{"yold", "=", "0"}], ",", 
      RowBox[{"x", "=", "0"}], ",", 
      RowBox[{"y", "=", "1"}], ",", 
      RowBox[{"c", "=", "1"}]}], "}"}], ",", 
    RowBox[{
     RowBox[{"Print", "[", 
      RowBox[{"\"\<The GCD of \>\"", ",", "a2", ",", "\"\< and \>\"", ",", 
       "b2", ",", "\"\< is \>\"", " ", ",", 
       RowBox[{"GCD", "[", 
        RowBox[{"a2", ",", "b2"}], "]"}], ",", "\"\<.\>\"", 
       ",", "\"\<\\nWe want to find integer solutions of \>\"", ",", "a2", 
       ",", "\"\< x +\>\"", ",", "b2", ",", "\"\< y = \>\"", ",", 
       RowBox[{"GCD", "[", 
        RowBox[{"a2", ",", "b2"}], "]"}], ",", "\"\<.\>\""}], 
      StyleBox["]",
       FontSlant->"Plain"]}], ";", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"c", "\[NotEqual]", "0"}], ",", 
       RowBox[{
        RowBox[{"q", "=", 
         RowBox[{"IntegerPart", "[", 
          RowBox[{"a2", "/", "b2"}], "]"}]}], ";", 
        RowBox[{"c", "=", 
         RowBox[{"a2", "-", 
          RowBox[{"q", " ", "b2"}]}]}], ";", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"IntegerQ", "[", "c", "]"}], ",", 
          RowBox[{"Print", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"a1", " ", "x"}], "+", 
             RowBox[{"b1", " ", "y"}]}], ",", "\"\< = \>\"", ",", "a1", 
            ",", "\"\< \[CenterDot] (\>\"", ",", "x", ",", "\"\<) + \>\"", ",",
             "b1", ",", "\"\< \[CenterDot] (\>\"", ",", "y", 
            ",", "\"\<)\>\""}], "]"}], ","}], "]"}], ";", 
        RowBox[{"xnew", "=", 
         RowBox[{"xold", "-", 
          RowBox[{"q", " ", "x"}]}]}], ";", 
        RowBox[{"ynew", "=", 
         RowBox[{"yold", "-", 
          RowBox[{"q", " ", "y"}]}]}], ";", 
        RowBox[{"xold", "=", "x"}], ";", 
        RowBox[{"yold", "=", "y"}], ";", 
        RowBox[{"x", "=", "xnew"}], ";", 
        RowBox[{"y", "=", "ynew"}], ";", 
        RowBox[{"a2", "=", "b2"}], ";", 
        RowBox[{"b2", "=", "c"}]}]}], "]"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.5564149597922745`*^9, 3.5564150581036477`*^9}, {
