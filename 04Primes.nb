Cell[BoxData[
 RowBox[{
  RowBox[{"ListPrimes", "[", "n_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"k", "=", "1"}], ",", 
      RowBox[{"list", "=", 
       RowBox[{"{", "}"}]}]}], "}"}], ",", 
    RowBox[{
